{
  "manifest_version": 3,
  "name": "Behaviour Tracker CAPTCHA",
  "version": "1.3",
  "description": "Tracks human behavior and triggers CAPTCHA on suspicious activity.",
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_title": "Activity Tracker"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "captcha-puzzle.html",
        "captcha-text.html",
        "captcha-puzzle.js",
        "captcha-text.js",
        "captcha-puzzle.css",
        "captcha-text.css"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}
